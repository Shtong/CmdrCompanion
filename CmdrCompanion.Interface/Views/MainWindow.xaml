<Window x:Class="CmdrCompanion.Interface.Views.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:v="clr-namespace:CmdrCompanion.Interface.Views"
        Title="Commander Companion" Height="600" Width="800"
        DataContext="{Binding Main, Source={StaticResource Locator}}" >
    <TabControl>
        <TabItem Header="Data Sheets">
            <v:DataSheetsHome />
        </TabItem>
        <TabItem Header="Travel Designer">
            <v:TravelDesigner />
        </TabItem>
        <TabItem Header="Jump finder">
            <v:JumpFinder />
        </TabItem>
        <TabItem Header="Options">
            <v:Options />
        </TabItem>
    </TabControl>
</Window>
